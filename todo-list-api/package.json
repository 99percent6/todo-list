    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.17.2",
    "bodybuilder": "^2.2.17",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.4",
    "firebase": "^5.9.4",
    "lodash": "^4.17.14",
    "mysql": "^2.17.1",
    "redis": "^2.8.0",
