    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "nodemon": "^1.9.2",
    "webpack": "^4.30.0"
